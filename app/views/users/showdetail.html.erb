
<% @addresses.each do |address| %>
<p id="notice"><%= notice %></p>

<table border=1>
<tr><td>Address 1:</td><td><%= address.address %></td></tr>
<tr><td>Address 2:</td><td><%= address.city %></td></tr>
<tr><td>County:</td><td><%= address.county %></td></tr>
<tr><td>Country:</td><td><%= address.country %></td></tr>
</table>

<% end %>
